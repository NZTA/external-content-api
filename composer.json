{
    "name": "nzta/external-content-api",
    "description": "Module for managing external content for NZTA API's",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "new zealand",
        "nzta",
        "api"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Charm Quarks",
            "email": "charmquarks@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/admin": "^1",
        "silverstripe/restfulserver": "^2",
        "silverstripe/versioned": "^1",
        "silverstripe/versioned-admin": "^1"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "NZTA\\ContentApi\\": "src/",
            "NZTA\\ContentApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
